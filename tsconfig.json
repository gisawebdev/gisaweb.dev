{
	"extends": "astro/tsconfigs/base",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@assets/*": ["src/assets/*"],
			"@config": ["src/config.ts"],
			"@components/*": ["src/components/*"],
			"@content/*": ["src/content/*"],
			"@layouts/*": ["src/layouts/*"],
			"@pages/*": ["src/pages/*"],
			"@utils/*": ["src/utils/*"]
		}
	}
}
